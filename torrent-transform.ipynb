   "source": [
    "objTorrentFile = open(\"file/*.torrent\", \"rb\").read()\n",
    "decodedDict = bencoding.bdecode(objTorrentFile)\n",
    "\n",
    "info_hash = hashlib.sha1(bencoding.bencode(decodedDict[b\"info\"])).hexdigest()\n",
    "print(\"magnet:?xt=urn:btih:{}\".format(info_hash))"
   ]
  }
